<?php
/// ThesisVO
class ThesisVO
{
	public $thesis_id;
	public $default_native_title_id;
	public $lecturer_id;
	public $academic_year;
	public $bachelor_approval_by;
	public $cathedra_approval_by;
	public $institution_id;
	public $default_english_title_id;
	
	public function __construct(
		$thesis_id, $default_native_title_id, $lecturer_id, $academic_year, $bachelor_approval_by, $cathedra_approval_by, $institution_id, $default_english_title_id)
	{
		$this->thesis_id = $thesis_id;
		$this->default_native_title_id = $default_native_title_id;
		$this->lecturer_id = $lecturer_id;
		$this->academic_year = $academic_year;
		$this->bachelor_approval_by = $bachelor_approval_by;
		$this->cathedra_approval_by = $cathedra_approval_by;
		$this->institution_id = $institution_id;
		$this->default_english_title_id = $default_english_title_id;
	}
}
class StudentThesesVO
{
	public $student_id;
	public $thesis_id;
	public $apply_date;
	public $native_title_id;
	public $english_title_id;
	public $student_approved;
	
	public function __construct($student_id, $thesis_id, $apply_date, $native_title_id, $english_title_id, $student_approved)
	{
		$this->student_id =$student_id;
		$this->thesis_id =$thesis_id;
		$this->apply_date =$apply_date;
		$this->native_title_id =$native_title_id;
		$this->english_title_id =$english_title_id;
		$this->student_approved =$student_approved;
	}
}
